<?xml version="1.0" encoding="UTF-8"?>  
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<typeAlias type="com.family.framework.customer.vo.CustomerModel" alias="CM"/>
		<typeAlias type="com.family.framework.customer.vo.CustomerQueryModel" alias="CQM"/>
	</typeAliases>
	<plugins>
		<plugin interceptor="com.family.framework.common.pageutil.PagePlugin">
			<property name="dialect" value="mysql"/><!-- 数据库类型 -->
			<property name="pageSqlId" value=".*Page.*"/><!-- 拦截page的方法 -->
		</plugin>
	</plugins>
</configuration> 